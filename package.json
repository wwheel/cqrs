{
  "name": "cqrs",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build cqrs --prod",
    "copy-license": "cp ./LICENSE ./dist/cqrs",
    "copy-readme": "cp ./README.md ./dist/cqrs",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/cqrs && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "publish": "npm run package && cd dist/cqrs && npm publish --access=public"
  },
  "author": {
    "name": "ivkan",
    "email": "ivan.kalashnik@immerss.com"
  },
  "license": "ISC",
  "private": false,
  "dependencies": {
    "@angular/animations": "11.0.9",
    "@angular/common": "11.0.9",
    "@angular/compiler": "11.0.9",
    "@angular/core": "11.0.9",
    "@angular/forms": "11.0.9",
    "@angular/platform-browser": "11.0.9",
    "@angular/platform-browser-dynamic": "11.0.9",
    "@angular/router": "11.0.9",
    "rxjs": "6.6.3",
    "tslib": "2.1.0",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.7",
    "@angular/cli": "11.0.7",
    "@angular/compiler-cli": "11.0.9",
    "@types/jasmine": "3.6.2",
    "@types/node": "14.14.20",
    "codelyzer": "6.0.1",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "^11.0.0",
    "protractor": "7.0.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.0.5"
  }
}
